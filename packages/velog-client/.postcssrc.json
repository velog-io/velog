{
  "plugins": [
    "postcss-modules",
    "postcss-modules-values",
    "postcss-import",
    "postcss-custom-media",
    "postcss-flexbugs-fixes",
    [
      "postcss-preset-env",
      {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3,
        "features": {
          "custom-properties": false,
          "custom-media-queries": true
        }
      }
    ]
  ]
}